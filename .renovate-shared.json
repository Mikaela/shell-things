{
  "extends": [
    "config:recommended",
    "group:allDigest"
  ],
  "git-submodules": {
    "enabled": true
  },
  "mode": "full",
  "packageRules": [
    {
      "automerge": false,
      "matchPackagePatterns": [
        "*"
      ]
    }
  ],
  "pre-commit": {
    "enabled": true
  },
  "schedule": [
    "before 6am"
  ],
  "timezone": "Europe/Helsinki"
}
